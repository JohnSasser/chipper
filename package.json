{
  "name": "chipper",
  "version": "1.0.0",
  "description": "Pet Finder Databasing between vets and pet owners",
  "main": "server.js",
  "dependencies": {
    "aws-sdk": "^2.329.0",
    "axios": "^0.21.1",
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "concurrently": "^4.0.1",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "^4.16.3",
    "express-flash": "0.0.2",
    "express-session": "^1.17.1",
    "http-errors": "~1.6.3",
    "if-env": "^1.0.4",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.10",
    "morgan": "^1.10.0",
    "multer": "^1.4.0",
    "multer-s3": "^2.7.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "react-router-dom": "^5.1.2",
    "rxjs": "^6.5.5",
    "url": "^0.11.0",
    "validator": "^10.8.0"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seedAll": "node scripts/seedDB.js && node scripts/seedAdminKeys.js",
    "seedDB": "node scripts/seedDB.js",
    "seedKeys": "node scripts/seedAdminKeys.js",
    "removeEverything": "node scripts/removeEverything.js",
    "removeAllPets": "node scripts/removeAllPets.js",
    "removeAllPosts": "node scripts/removeAllPosts.js",
    "removeAllUsers": "node scripts/removeAllUsers.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build",
    "server": "nodemon server.js",
    "alanclient": "npm start --prefix client",
    "alanaws": "concurrently \"npm run alanserver\" \"npm run alanclient\""
  },
  "author": "Alan Margolies, Austin Atkinson, John Sasser, Rami Forde",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JohnSasser/CHIPPER.git"
  },
  "bugs": {
    "url": "https://github.com/JohnSasser/CHIPPER/issues"
  },
  "homepage": "https://github.com/JohnSasser/CHIPPER#readme"
}
